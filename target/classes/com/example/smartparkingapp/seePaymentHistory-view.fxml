<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #1a1a1a;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.controllers.SeePaymentHistoryController">
    <children>
      <ImageView fitHeight="400.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true" style="-fx-opacity: 10%;">
         <image>
            <Image url="@../../../Images/DepositPage/Background.png" />
         </image>
      </ImageView>
        <Button fx:id="previousPageButton" layoutX="14.0" layoutY="13.0" mnemonicParsing="false" onAction="#onPreviousPageButtonClick" prefWidth="160.0" style="-fx-background-color: transparent;" text="PREVIOUS PAGE" textFill="WHITE">
         <graphic>
            <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../Images/DepositPage/icons8-back-24.png" />
               </image>
            </ImageView>
         </graphic></Button>
        <Label fx:id="usernameLabel" layoutX="448.0" layoutY="21.0" prefHeight="17.0" prefWidth="138.0" text="Label" textFill="WHITE" />
        <ListView fx:id="paymentHistory" layoutX="14.0" layoutY="72.0" prefHeight="289.0" prefWidth="200.0" />
        <ListView fx:id="dateList" layoutX="214.0" layoutY="72.0" prefHeight="289.0" prefWidth="200.0" />
        <Text fill="WHITE" layoutX="80.0" layoutY="59.0" strokeType="OUTSIDE" strokeWidth="0.0" text="VALUE(RON)" />
        <Text fill="WHITE" layoutX="300.0" layoutY="59.0" strokeType="OUTSIDE" strokeWidth="0.0" text="DATE" />
        <Label fx:id="errorLabel" layoutX="14.0" layoutY="365.0" prefHeight="17.0" prefWidth="200.0" style="-fx-alignment: center;" />
    </children>
</AnchorPane>
